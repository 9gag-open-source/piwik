{
  "config": {
    "type": "Config@config"
  },
  "app": {
    "type": "Application@app"
  },
  "page-renderer": {
    "type": "PageRenderer@page-renderer"
  },
  "chai-loader": {
    "type": "ChaiLoader@chai-loader",
    "construct": [
      "@page-renderer"
    ]
  },
  "mocha-loader": {
    "type": "MochaLoader@mocha-loader",
    "construct": [
      "@config"
    ]
  },
  "resemble-loader": {
    "type": "ResembleLoader@resemble-loader"
  }
}